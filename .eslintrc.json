{
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "browser": true
  },
  "extends": ["airbnb", "prettier"],
  "rules": {
    "react/prop-types": false,
    "react/destructuring-assignment": false,
    "react/button-has-type": false,
    "no-useless-constructor": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
